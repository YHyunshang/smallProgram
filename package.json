{
  "name": "myApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --host 0.0.0.0 --port 8081",
    "clean": "rm -rf dist-ios && rm -rf dist-android",
    "bundle-android": "rm -rf dist-android && mkdir dist-android && react-native bundle --entry-file index.js --bundle-output ./dist-android/index.jsbundle --platform android --assets-dest ./dist-android --dev false",
    "bundle-ios": "rm -rf dist-ios && mkdir dist-ios && react-native bundle --entry-file index.js --bundle-output ./dist-ios/index.jsbundle  --platform ios --assets-dest ./dist-ios --dev false",
    "bundle": "npm run bundle-ios && npm run bundle-android",
    "lint": "eslint . --fix --format=node_modules/eslint-formatter-pretty --quiet"
  },
  "dependencies": {
    "react": "16.8.3",
    "react-native": "^0.59.9",
    "react-native-fit-image": "^1.5.4",
    "react-native-fs": "^2.14.1",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-swiper": "^1.5.14",
    "react-native-vector-icons": "^6.6.0",
    "react-native-wechat": "^1.9.10",
    "rn-fetch-blob": "^0.10.16"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/runtime": "7.4.5",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.60.2",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "24.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "console": "^0.7.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-native": "^3.7.0",
    "jest": "24.8.0",
    "lodash": "^4.17.15",
    "metro-react-native-babel-preset": "0.54.1",
    "react-test-renderer": "16.8.3",
    "tslint": "^5.18.0"
  },
  "jest": {
    "preset": "react-native"
  }
}